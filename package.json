{
  "name": "configuracion-directorios-empresariales-gemini",
  "private": true,
  "version": "1.7.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run sync-version && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "version": "node check-version.js",
    "update-date": "node scripts/update-build-date.js",
    "sync-version": "node scripts/sync-version.js",
    "auto-version": "node scripts/auto-version.js",
    "update-version": "node scripts/update-version.js",
    "version-patch": "npm version patch --no-git-tag-version && npm run sync-version",
    "version-minor": "npm version minor --no-git-tag-version && npm run sync-version",
    "version-major": "npm version major --no-git-tag-version && npm run sync-version",
    "auto-commit": "npm run sync-version && git add . && git commit",
    "setup-hooks": "node scripts/setup-hooks.js",
    "pre-commit": "npm run sync-version"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "date-fns": "^2.30.0",
    "papaparse": "^5.4.1",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}