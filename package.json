{
  "name": "configuracion-directorios-empresariales-gemini",
  "private": true,
  "version": "1.7.67",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sync-version": "node scripts/sync-version.js",
    "auto-version": "node scripts/auto-version.js",
    "setup-hooks": "node scripts/setup-hooks.js",
    "version-patch": "npm version patch && npm run sync-version",
    "version-minor": "npm version minor && npm run sync-version",
    "version-major": "npm version major && npm run sync-version",
    "auto-commit": "npm run sync-version && git add . && git commit",
    "force-version-sync": "node scripts/force-version-sync.js",
    "verify-sync": "node scripts/verify-sync.js",
    "auto-update-version": "node scripts/auto-update-version.js",
    "setup-git-hooks": "node scripts/setup-git-hooks.js",
    "commit-with-version": "npm run auto-update-version && git commit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "zustand": "^4.4.1",
    "@supabase/supabase-js": "^2.39.0",
    "pusher-js": "^8.2.0",
    "papaparse": "^5.4.1",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/papaparse": "^5.3.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
